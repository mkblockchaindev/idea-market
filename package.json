{
  "name": "ideamarket-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --max-warnings=0 --ext .ts,.tsx --ignore-path .gitignore .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "prepare": "husky install",
    "apply-db-migrations": "yarn fauna-schema-migrate apply all"
  },
  "dependencies": {
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@ethersproject/solidity": "^5.5.0",
    "@headlessui/react": "1.4.2",
    "@heroicons/react": "1.0.5",
    "@next-auth/fauna-adapter": "0.2.4",
    "@portis/web3": "^4.0.6",
    "@sentry/nextjs": "^6.15.0",
    "@tailwindcss/aspect-ratio": "0.3.0",
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/typography": "0.4.1",
    "@types/chart.js": "^2.9.34",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.9",
    "@types/uuid": "8.3.3",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v3-periphery": "^1.2.1",
    "@uniswap/v3-sdk": "^3.5.1-optimism-regensis",
    "@walletconnect/ethereum-provider": "^1.6.6",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "6.2.8",
    "@web3-react/walletlink-connector": "^6.2.8",
    "airtable": "0.11.1",
    "autoprefixer": "10.4.0",
    "aws-sdk": "2.1034.0",
    "axios": "^0.24.0",
    "bignumber.js": "9.0.1",
    "chart.js": "^2.9.4",
    "cheerio": "1.0.0-rc.10",
    "classnames": "2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "fauna-schema-migrate": "2.1.2",
    "faunadb": "4.4.1",
    "fortmatic": "^2.2.1",
    "google-trends-api": "4.9.2",
    "graphql": "16.0.1",
    "graphql-request": "3.6.1",
    "immer": "9.0.6",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.42.0",
    "next": "12.0.4",
    "next-auth": "3.29.0",
    "next-pwa": "^5.4.1",
    "next-seo": "4.28.1",
    "next-themes": "^0.0.15",
    "nprogress": "0.2.0",
    "numeral": "^2.0.6",
    "postcss": "8.3.11",
    "rc-slider": "^9.7.4",
    "react": "17.0.2",
    "react-cookie-consent": "^6.4.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.20.2",
    "react-hot-toast": "2.1.1",
    "react-icons": "^4.3.1",
    "react-input-verification-code": "^0.8.0",
    "react-query": "2.26.2",
    "react-select": "5.2.1",
