    "husky": "^7.0.4",
    "lint-staged": ">=12.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.1",
    "tailwindcss": "2.2.19",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "yarn run lint"
    ],
    "*.+(js|json|ts|tsx)": [
      "yarn run format"
    ]
  }
}
